{
  "name": "pebblebed",
  "version": "0.2.0",
  "description": "Simplified interactions with Google Datastore for NodeJS",
  "main": "dist-es6/index.js",
  "types": "dist-es6/index.d.ts",
  "jsnext:main": "dist/index.js",
  "jsnext:types": "dist/index.d.ts",
  "engines": {
    "node": ">=6.5.0"
  },
  "scripts": {
    "build": "concurrently \"npm run build-es2017\" \"npm run build-es6\"",
    "watch": "concurrently \"npm run watch-es2017\" \"npm run watch-es6\"",
    "build-es2017": "tsc",
    "build-es6": "tsc --p tsconfig.es6.json",
    "watch-es6": "tsc --p tsconfig.es6.json --watch",
    "watch-es2017": "tsc --watch",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lostpebble/pebblebed.git"
  },
  "keywords": [
    "google",
    "cloud",
    "datastore",
    "nodejs",
    "entities",
    "query",
    "load",
    "save"
  ],
  "author": "Paul Myburgh",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lostpebble/pebblebed/issues"
  },
  "homepage": "https://github.com/lostpebble/pebblebed#readme",
  "peerDependencies": {
    "@google-cloud/datastore": "^1.0.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.65",
    "@types/node": "^7.0.12",
    "lodash": "^4.17.4",
    "ts-node": "^3.0.2",
    "typescript": "^2.2.2",
    "concurrently": "^3.4.0"
  }
}
