{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "allowJs": false,
    "experimentalDecorators": true,
    "sourceMap": true,
    "inlineSourceMap": false,
    "declaration": true,
    "outDir": "dist",
    "strictNullChecks": true,
    "rootDir": "./src",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  },
  "exclude": [
    "dist",
    "test",
    "gatsby-site",
    "site",
    "node_modules"
  ]
}
